package mum.waa.model;

import java.io.Serializable;

import javax.enterprise.context.SessionScoped;
import javax.inject.Named;

@Named("tableData")
@SessionScoped
public class TableData implements Serializable {

	private static final Name[] names = new Name[] { new Name("William", "Dupont"), new Name("Anna", "Keeney"),
			new Name("Mariko", "Randor"), new Name("John", "Wilson"), new Name("Dipen", "Lama"),
			new Name("Jai", "Limbu"), new Name("Sanjay", "Shrestha") };

	public Name[] getNames() {
		return names;
	}

	public String save() {
		for (Name name : names)
			name.setEditable(false);
		return null;
	}

	public String deleteRow(Name nameToDelete) {
		
		for (Name name : names) {
			if(nameToDelete.getFirst().equals(name.getFirst()))
			{}
		}
		return null;
	}

}
